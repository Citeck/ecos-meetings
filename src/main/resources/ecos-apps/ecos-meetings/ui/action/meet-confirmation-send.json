{
  "id": "meet-confirmation-send",
  "name": {
    "ru": "Рассылка на согласование",
    "en": "Sending for confirmation"
  },
  "type": "create",
  "config": {
    "formId": "uiserv/form@meet_confirmation_send_task",
    "redirectToPage": false
  },
  "evaluator": {
    "type": "predicate",
    "config": {
      "predicate": {
        "t": "and",
        "val" : [
          {
            "t": "eq",
            "att": "_status?str",
            "val": "conducted"
          },
          {
            "t": "not-eq",
            "att": "childProtocol?str",
            "val": "null"
          }
        ]
      }
    }
  },
  "features": {
    "execForRecord": true,
    "execForRecords": false
  }
}
