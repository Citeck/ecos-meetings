{
  "id": "meet-create-protocol",
  "name": {
    "ru": "Создать протокол",
    "en": "Create protocol"
  },
  "type": "create",
  "config": {
    "typeRef": "emodel/type@meet-protocol",
    "formId": "uiserv/form@meet_protocol",
    "redirectToPage": false,
    "attributes": {
      "childProtocol":"${recordRef}"
    }
  },
  "evaluator": {
    "type": "predicate",
    "config": {
      "predicate": {
        "t": "and",
        "val" : [
          {
            "t": "eq",
            "att": "_status?str",
            "val": "conducted"
          },
          {
            "t": "eq",
            "att": "childProtocol?str",
            "val": "null"
          }
        ]
      }
    }
  },
  "features": {
    "execForRecord": true,
    "execForRecords": false
  }
}
