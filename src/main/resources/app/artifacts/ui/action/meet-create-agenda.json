{
  "id": "meet-create-agenda",
  "name": {
    "ru": "Создать повестку совещания",
    "en": "Create agenda"
  },
  "type": "create",
  "config": {
    "typeRef": "emodel/type@meet-agenda",
    "formId": "uiserv/form@meet_agenda",
    "redirectToPage": false,
    "attributes": {
      "childAgenda":"${recordRef}"
    }
  },
  "evaluator": {
    "type": "predicate",
    "config": {
      "predicate": {
        "t": "and",
        "val" : [
          {
            "t": "eq",
            "att": "_status?str",
            "val": "new"
          },
          {
            "t": "eq",
            "att": "childAgenda?str",
            "val": "null"
          }
        ]
      }
    }
  },
  "features": {
    "execForRecord": true,
    "execForRecords": false
  }
}
